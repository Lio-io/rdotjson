{
  "name": "rdotjson",
  "description": "Android String Resource XML Parser",
  "version": "0.1.2",
  "author": "@kawanet",
  "bin": {
    "rdotjson": "./rdotjson.cli.js"
  },
  "bugs": {
    "url": "https://github.com/kawanet/rdotjson/issues"
  },
  "dependencies": {
    "cheerio": "^0.20.0",
    "is-stream": "^1.1.0",
    "process.argv": "^0.1.0"
  },
  "devDependencies": {
    "jshint": "^2.9.2",
    "mocha": "^2.5.3"
  },
  "homepage": "https://github.com/kawanet/rdotjson#readme",
  "jshintConfig": {
    "eqnull": true,
    "node": true,
    "undef": true
  },
  "keywords": [
    "Android",
    "CSV",
    "JSON",
    "Java",
    "R",
    "XML"
  ],
  "license": "MIT",
  "main": "./rdotjson.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kawanet/rdotjson.git"
  },
  "scripts": {
    "fixpack": "fixpack",
    "jshint": "./node_modules/.bin/jshint *.js *.json format/*.js gist/*.js model/*.js test/*.js",
    "mocha": "./node_modules/.bin/mocha -R spec test/*.js",
    "test": "npm run jshint && npm run mocha"
  }
}
